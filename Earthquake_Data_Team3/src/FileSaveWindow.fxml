<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.FileSaveWindowController">
   <children>
      <Button fx:id="saveButton" layoutX="262.0" layoutY="232.0" mnemonicParsing="false" onAction="#saveButtonAction" prefHeight="54.0" prefWidth="181.0" text="Save">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Label alignment="CENTER" layoutX="106.0" layoutY="14.0" prefHeight="54.0" prefWidth="290.0" text="Choose a Filename:" textAlignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="cancelButton" layoutX="46.0" layoutY="232.0" mnemonicParsing="false" onAction="#cancelButtonAction" prefHeight="54.0" prefWidth="181.0" text="Cancel">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <TextField fx:id="fileNameField" layoutX="38.0" layoutY="126.0" prefHeight="48.0" prefWidth="425.0" />
   </children>
</AnchorPane>
